def simple(): Unit = {
  println("Simple")
}

simple()

def adder(num1:Int , num2:Int) : Int = {
  return num1 + num2
}

adder(4,5)

def greetName(name:String ): String ={
  return s"Hello $name"
}

greetName("Avinash")

def Isprime(numcheck: Int): Boolean ={
  for (n <- Range(2, numcheck) ){
    if(numcheck%n == 0)
    {
      return false
    }
  }
  return true
}



print(Isprime(10))
print(Isprime(23))
